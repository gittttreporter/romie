    "eslint-plugin-vue": "^10.6.2",
    "git-cliff": "^2.11.0",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "less": "^4.5.1",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
