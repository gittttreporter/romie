{
  "name": "romie",
  "productName": "ROMie",
  "version": "0.5.0-rc.4",
  "description": "ROM Manager for Retro Handhelds",
  "main": ".vite/build/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/JZimz/romie.git"
  },
  "bugs": {
    "url": "https://github.com/JZimz/romie/issues"
  },
  "homepage": "https://github.com/JZimz/romie#readme",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "postinstall": "node scripts/postinstall.mjs",
    "build:game-db": "tsx scripts/build-game-db",
    "lint": "npm run lint:eslint && npm run lint:types && npm run format:check",
    "lint:eslint": "eslint . --ext .js,.cjs,.mjs,.ts,.vue --max-warnings=0",
    "lint:types": "vue-tsc --noEmit",
    "lint:fix": "eslint . --ext .js,.cjs,.mjs,.ts,.vue --fix && npm run format",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest run",
    "prepare": "husky",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "version:patch": "npm version patch -m \"chore(release): %s\"",
    "version:minor": "npm version minor -m \"chore(release): %s\"",
    "version:major": "npm version major -m \"chore(release): %s\""
  },
  "keywords": [
    "rom-manager",
    "retro-gaming",
    "emulation",
    "miyoo-mini",
    "miyoo-mini-plus",
    "handheld",
    "sd-card-sync",
    "game-library",
    "retroachievements",
    "electron-app",
    "onion-os",
    "portable-gaming"
  ],
  "author": {
    "name": "JZimz",
    "email": "jzimz.tv@gmail.com"
  },
  "license": "MIT",
